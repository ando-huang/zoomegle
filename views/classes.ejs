<!DOCTYPE html>
    <head>
        <meta charset="utf-8">

        <title>Zoomegle</title>
        <meta name="description" content="The HTML5 Herald">
        <meta name="author" content="SitePoint">
    </head>

    <body>

      <table>
        <thead>
          <tr>
            <th>#</th>
            <th>Class Name</th>
            <th>Link</th>
          </tr>
        </thead>
        <tbody>
          <% for(var i = 0; i < classes.length; i++) { %>
            <tr>
              <th><%= i + 1 %></th>
              <td><%= classes[i].name %></td>
              <td><a href="<%= classes[i].link %>">link</a></td>
            </tr>
          <% } %>
        </tbody>
      </table>

    </body>

</html>